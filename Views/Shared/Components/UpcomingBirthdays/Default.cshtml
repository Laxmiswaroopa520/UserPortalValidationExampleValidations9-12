@model IEnumerable<UserPortalValdiationsDBContext.Models.User>

<h6>Upcoming Birthdays</h6>
<ul class="list-group">
    @foreach (var user in Model)
    {
    <li class="list-group-item">
        @user.Username - @(user.DateOfBirth.HasValue
                ? user.DateOfBirth.Value.ToString("MMM dd")
                : "N/A")
        </li>
    }
</ul>
